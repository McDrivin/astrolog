<div class="container">
  <h1><%= @launch.name %></h1>
  <%= image_tag @launch.feature_image %>
  <p><%= @launch.description %></p>
  <%= link_to "News", "#{@launch.news_url}", class: "btn btn-success" %>
  <%= link_to "Video", "#{@launch.video_url}", class: "btn btn-success" %>
</div>

<%#
launches.each do |launch|
  Launch.create!(
    name: launch["name"],
    status: launch["status"]["name"],
    status_desc: launch["status"]["description"],
    window_start: launch["window_start"],
    window_end: launch["window_end"],
    probability: launch["probability"],
    agenc: launch["launch_service_provider"]["name"],
    rocket: launch["rocket"]["configuration"]["name"],
    mission: launch["mission"]["name"],
    pad: launch["pad"]["name"],
    pad_lat: launch["pad"]["latitude"],
    pad_lng: launch["pad"]["longitude"]
  )
end %>
